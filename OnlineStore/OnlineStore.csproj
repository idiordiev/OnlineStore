      <Compile Update="Resources\Models\Goods.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>Goods.resx</DependentUpon>
      </Compile>
      <Compile Update="Resources\Models\Goods.ru.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
